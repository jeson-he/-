#include<stdio.h>
void fact(int a[],int n)
{
	int i,j,k;
	for(i=0;i<n-1;i++)
	{
		for(j=0;j<n-1-i;j++)
		{
			if(a[j]>a[j+1])
			{
				k=a[j];
				a[j]=a[j+1];
				a[j+1]=k;
			}
		}
	}
}


int main()
{
	int num[]={8,7,6,5};
	int i;
	fact(num,4);
	for(i=0;i<4;i++)
	{
		printf("%d ",num[i]);
	}
	printf("\n");
	return 0;	
	
}
